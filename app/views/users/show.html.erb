<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %>
        Welcome <%= @user.name %>
      </h1>
    </section>
  </aside>
  <section class="col-md-8">
    <div class="row">
      <section class="col-md-12">
        <%= @user.name %>
      </section>
      <article class="col-md-12">
        <%= @user.name %>
      </article>
    </div>
  </section>
</div>
